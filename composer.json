{
  "name": "heyday/silverstripe-slices",
  "description": "Provides a Slice extension to DataObject subclasses with custom templates",
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "require": {
    "silverstripe/cms": "^5",
    "symbiote/silverstripe-gridfieldextensions": "^4"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Heyday\\SilverStripeSlices\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "3.0.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
